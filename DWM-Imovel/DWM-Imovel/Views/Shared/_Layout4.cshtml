<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>DWM-Imóvel</title>

    <!-- Typeahead CSS -->
    <link rel="stylesheet" href="~/Content/css/jquery.typeahead.css">

    <!-- Web Fonts -->
    <link rel="stylesheet" type="text/css" href="~/Content/Theme-DarkAdmin/font-awesome/css/font-awesome.min.css" />

    <!-- Datepicker CSS -->
    <link href="~/Scripts/css/datepicker.css" rel="stylesheet">

    <!-- Plugins -->
    <link href="~/Content/css/animations.css" rel="stylesheet">

    <!-- /bootstrap css -->
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Theme-DarkAdmin/css/local.css" />

    <!-- Custom Components - André Leal -->
    @*<link href="@Styles.Url("~/content/css")" rel="stylesheet" />
    <link href="@Styles.Url("~/scripts/css")" rel="stylesheet" />*@

    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.js"></script>
    <script type="text/javascript" src="~/scripts/jquery-{version}.js"></script>
    <script type="text/javascript" src="~/scripts/jquery-migrate-{version}.js"></script>
    <script type="text/javascript" src="~/scripts/jquery.validate.js"></script>
    <script type="text/javascript" src="~/scripts/js/bootstrap-datepicker.js"></script>
    <script type="text/javascript" src="~/scripts/jquery.validate.unobtrusive.js"></script>
    <script type="text/javascript" src="~/scripts/jquery.validate-fix.js"></script>
    <script type="text/javascript" src="~/scripts/js/locales/bootstrap-datepicker.pt-BR.js"></script>
    <script type="text/javascript" src="~/scripts/jquery.validate.unobtrusive-custom-for-bootstrap.js"></script>
    <script type="text/javascript" src="~/scripts/jquery.maskedinput.js"></script>
    <script type="text/javascript" src="~/scripts/inputs-jquery-ui.js"></script>
    <script type="text/javascript" src="~/Content/typeahead/jquery.typeahead.min.js"></script>

    <link href="@Styles.Url("~/Content/Site.css")" rel="stylesheet" />
    <link href="@Styles.Url("~/Content/bootstrap-mvc-validation.css")" rel="stylesheet" />

    <!-- JavaScript files placed at the end of the document so the pages load faster
    ================================================== -->
    <!-- Jquery and Bootstap core js files -->
    @*<script type="text/javascript" src="~/Content/bootstrap/js/bootstrap.min.js"></script>*@
    <script type="text/javascript" src="~/Content/Theme-DarkAdmin/bootstrap/js/bootstrap.min.js"></script>

    <!-- Modernizr javascript -->
    <script type="text/javascript" src="~/Content/plugins/modernizr.js"></script>

    <!-- you need to include the shieldui css and js assets in order for the charts to work -->
    <link rel="stylesheet" type="text/css" href="http://www.shieldui.com/shared/components/latest/css/shieldui-all.min.css" />
    <link rel="stylesheet" type="text/css" href="http://www.shieldui.com/shared/components/latest/css/light-bootstrap/all.min.css" />
    <link id="gridcss" rel="stylesheet" type="text/css" href="http://www.shieldui.com/shared/components/latest/css/dark-bootstrap/all.min.css" />
    <script type="text/javascript" src="http://www.shieldui.com/shared/components/latest/js/shieldui-all.min.js"></script>
    <script type="text/javascript" src="http://www.prepbootstrap.com/Content/js/gridData.js"></script>

</head>
<body>

    <div id="wrapper">

        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="../Home/Default">DWM-Imóvel</a>
            </div>

            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul id="active" class="nav navbar-nav side-nav">
                    @*<li><a href="../Home/Default"><i class="fa fa-bullseye"></i> Home</a></li>*@
                    <li><a href="../Empreendimentos/Browse"><i class="fa fa-tasks"></i> Empreendimentos</a></li>
                    <li><a href="../Clientes/Browse"><i class="fa fa-globe"></i> Clientes</a></li>
                    <li><a href="../Corretores/Browse"><i class="fa fa-list-ol"></i> Corretores</a></li>
                    @*<li><a href="register.html"><i class="fa fa-font"></i> Register</a></li>
                        <li><a href="timeline.html"><i class="fa fa-font"></i> Timeline</a></li>
                        <li><a href="forms.html"><i class="fa fa-list-ol"></i> Forms</a></li>
                        <li><a href="typography.html"><i class="fa fa-font"></i> Typography</a></li>
                        <li><a href="bootstrap-elements.html"><i class="fa fa-list-ul"></i> Bootstrap Elements</a></li>
                        <li><a href="bootstrap-grid.html"><i class="fa fa-table"></i> Bootstrap Grid</a></li>*@
                </ul>

                <ul class="nav navbar-nav navbar-right navbar-user">
                    <li class="dropdown messages-dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-envelope"></i> Mensagens <span class="badge">2</span> <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li class="dropdown-header">2 Novas Mensagens</li>
                            <li class="message-preview">
                                <a href="#">
                                    <span class="avatar"><i class="fa fa-bell"></i></span>
                                    <span class="message">RPA e RPG com valores incorretos</span>
                                </a>
                            </li>
                            <li class="divider"></li>
                            <li class="message-preview">
                                <a href="#">
                                    <span class="avatar"><i class="fa fa-bell"></i></span>
                                    <span class="message">Contabilização autorizada</span>
                                </a>
                            </li>
                            <li class="divider"></li>
                            <li><a href="#">Caixa de Entrada <span class="badge">2</span></a></li>
                        </ul>
                    </li>
                    <li class="dropdown user-dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> Leonardo Dias<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="#"><i class="fa fa-user"></i> Perfil</a></li>
                            <li><a href="#"><i class="fa fa-gear"></i> Configurações</a></li>
                            <li class="divider"></li>
                            <li><a href="../Account/LogOff"><i class="fa fa-power-off"></i> Sair</a></li>

                        </ul>
                    </li>
                    <li>
                        <form class="navbar-search">
                            <div class="input-group">
                                <input type="text" placeholder="Buscar" class="form-control" style="width: 80%">
                                <a class="btn btn-primary" href="#"><i class="glyphicon glyphicon-search"></i></a>
                            </div>
                        </form>
                    </li>
                </ul>
            </div>
        </nav>

        <div id="page-wrapper">
            <div id="carregando" style="visibility: hidden; height: 0px; background-repeat: no-repeat; z-index: 5; background-image: url(../Content/images/loader.gif); background-position-x: 50%; background-position-y: 200px">
                <label>Carregando...</label>
            </div>
            <div>
                <div class="container">
                    @BreadCrumbPage.Trace((App_Dominio.Component.BreadCrumb)ViewBag.BreadCrumb)
                    @Html.Partial("_alerts")
                    <div class="alert alert-dismissable alert-warning" id="alerta" style="visibility: hidden">
                        <a class="close" href="#" aria-hidden="true" onclick="$('#alerta').hide()">x</a>
                        <h4 class="alert" id="alerta-heading">Atenção!</h4>
                        <div id="MsgAlert"></div>
                    </div>
                </div>
                @RenderBody()
            </div>


        </div>

    </div>
    @Scripts.Render("~/scripts")
    @Scripts.Render("~/js")
    @RenderSection("scripts", required: false)

    <script type="text/javascript">
        $("#alerta").hide();
        $("#alerta").css("visibility", "visible");
    </script>

</body>
</html>


