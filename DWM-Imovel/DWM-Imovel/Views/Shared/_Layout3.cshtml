                        $("body").addClass("fixed-header-on");
                    } else {
                        $("body").removeClass("fixed-header-on");
                    }
                };
            });
        </script>*@


    <!-- jQuery -->
    <script src="~/Content/Start-Bootstrap/bower_components/jquery/dist/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="~/Content/Start-Bootstrap/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Content/Start-Bootstrap/bower_components/metisMenu/dist/metisMenu.min.js"></script>

    <!-- Morris Charts JavaScript -->
    <script src="~/Content/Start-Bootstrap/bower_components/raphael/raphael-min.js"></script>
    <script src="~/Content/Start-Bootstrap/bower_components/morrisjs/morris.min.js"></script>
    <script src="~/Content/Start-Bootstrap/js/morris-data.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="~/Content/Start-Bootstrap/dist/js/sb-admin-2.js"></script>

</body>
</html>
