@model System.Web.Mvc.PagedList<DWM.Models.Repositories.EmpreendimentoViewModel>

@{
    Layout = null;
}
@if (Model != null)
{
    <div class="row Scroll">
        <div class="col-md-12 column hidden-xs">
            <table class="table table-hover table-bordered table-condensed" style="cursor: pointer">
                <caption></caption>
                <thead class="corvo text-white">
                    <tr>
                        <th>Nome</th>
                        <th class="text-center">Ação</th>
                    </tr>
                </thead>
                @foreach (DWM.Models.Repositories.EmpreendimentoViewModel emp in Model)
                {
                    <tr>
                        <td class="col-md-9 col-xs-7 col-sm-8">
                            <p>@emp.nomeEmpreend</p>
                        </td>
                        <td class="col-md-3 col-sm-2 text-center">
                            <div class="btn-group">
                                <a class="btn btn-primary btn-sm" href="../Clientes/Edit?empreendimentoId=@emp.empreendimentoId"><i class="glyphicon glyphicon-edit"></i></a>
                                <a class="btn btn-danger btn-sm" href="../Clientes/Delete?empreendimentoId=@emp.empreendimentoId"><i class="glyphicon glyphicon-minus"></i></a>
                            </div>
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
    @Html.Partial("Paginacao", Model)
}
else
{
    @Html.Partial("_AcessoNegadoModal")
}




